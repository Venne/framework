{
	"name": "venne/framework",
	"description": "Simple and easy for use system for managing your websites.",
	"keywords": ["cms", "nette", "doctrine"],
	"license": ["BSD-3-Clause", "GPL-2.0", "GPL-3.0"],
	"authors": [
		{
			"name": "Josef Kříž",
			"homepage": "http://josef-kriz.cz"
		}
	],
	"require": {
		"php": ">=5.3.2",
		"nette/nette": "~2.1.1",
		"venne/package-manager": "~2.2.0@dev"
	},
	"require-dev": {
		"nette/tester": "~1.0.0"
	},
	"autoload": {
		"psr-4": {
			"Venne\\": "src/"
		}
	},
	"autoload-dev": {
		"psr-4": {
			"VenneTests\\": "tests/src/"
		}
	},
	"bin": ["bin/venne", "bin/venne.php"],
	"extra": {
		"branch-alias": {
			"dev-master": "2.2-dev"
		},
		"venne": {
			"version": "2.2.0",
			"require": {
				"venne.package-manager": "2.2.x"
			}
		}
	}
}