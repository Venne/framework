{
	"name": "venne/framework",
	"description": "Simple and easy for use system for managing your websites.",
	"keywords": ["cms", "nette", "doctrine"],
	"type": "composer-installer",
	"authors": [
		{
			"name": "Josef Kříž",
			"homepage": "http://josef-kriz.cz"
		}
	],
	"require": {
		"php" : ">=5.3.2",
		"nette/nette" : "2.0.*",
		"composer/composer" : "dev-master"
	},
	"autoload": {
		"files": ["Venne/loader.php"],
		"psr-0": { "Venne": "" }
	},
	"extra": {
		"branch-alias": {
			"dev-master": "2.0.x-dev"
		},
		"class": "Venne\\Module\\Composer\\Installers\\ModuleInstaller"
	},
	"minimum-stability": "dev"
}